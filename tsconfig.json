{
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": "src",
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "src"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
